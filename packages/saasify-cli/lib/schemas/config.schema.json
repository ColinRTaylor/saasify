{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Config",
  "definitions": {
    "Config": {
      "title": "Config",
      "type": "object",
      "properties": {
        "saasifyVersion": {
          "type": "number",
          "title": "saasifyVersion",
          "default": 1
        },
        "version": {
          "type": "string",
          "title": "version",
          "default": "0.1.0"
        },
        "name": {
          "type": "string",
          "title": "name"
        },
        "name": {
          "type": "string",
          "title": "description"
        },
        "alias": {
          "type": "string",
          "title": "alias"
        },
        "saas": {
          "type": "object",
          "title": "saas",
          "additionalProperties": true
        },
        "authRateLimit": {
          "$ref": "#/definitions/RateLimit",
          "title": "authRateLimit"
        },
        "noAuthRateLimit": {
          "$ref": "#/definitions/RateLimit",
          "title": "noAuthRateLimit"
        },
        "amountPerBase": {
          "type": "number",
          "title": "amountPerBase"
        },
        "amountPerRequest": {
          "type": "number",
          "title": "amountPerRequest"
        },
        "amountPerCompute": {
          "type": "number",
          "title": "amountPerCompute"
        },
        "amountPerBandwidth": {
          "type": "number",
          "title": "amountPerBandwidth"
        },
        "services": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Service"
          },
          "title": "services"
        }
      },
      "additionalProperties": false,
      "required": [
        "services"
      ]
    },
    "Example": {
      "title": "Example",
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "title": "name"
        },
        "input": {
          "type": "object",
          "title": "input"
        },
        "output": {
          "title": "output",
          "oneOf": [
            {
              "type": "object"
            },
            {
              "type": "string"
            }
          ]
        },
        "additionalProperties": false
      },
      "required": [
        "name",
        "input"
      ]
    },
    "Service": {
      "title": "Service",
      "type": "object",
      "properties": {
        "src": {
          "type": "string",
          "title": "src"
        },
        "name": {
          "type": "string",
          "title": "name"
        },
        "timeout": {
          "type": "number",
          "title": "timeout",
          "default": 0
        },
        "examples": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Example"
          },
          "title": "examples"
        },
        "additionalProperties": false
      },
      "required": [
        "src"
      ]
    },
    "RateLimit": {
      "title": "RateLimit",
      "type": "object",
      "properties": {
        "requests": {
          "type": "boolean",
          "title": "requests",
          "default": true
        },
        "requestsInterval": {
          "type": "number",
          "title": "requestsInterval",
          "default": 60000
        },
        "requestsMaxPerInterval": {
          "type": "number",
          "title": "requestsMaxPerInterval",
          "default": 1000
        }
      },
      "additionalProperties": false,
      "required": [ ]
    }
  }
}
