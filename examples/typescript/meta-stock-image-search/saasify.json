    "theme": {
      "name": "waves",
      "backgroundImage": "https://images.unsplash.com/photo-1549916571-9b2c630403dc?ixlib=rb-1.2.1&auto=format&fit=crop&w=2550&q=80",
      "buttonStyle": "rounded",
      "color": "#4aaef7",
