{
  "name": "meta-stock-image-search",
  "description": "TODO",
  "alias": "meta-stock-image-search",
  "services": [
    {
      "src": "src/search-images.ts",
      "examples": [
        {
          "name": "Search for images of cats",
          "input": {
            "query": "cats"
          }
        }
      ]
    }
