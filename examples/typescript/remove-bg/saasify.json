{
  "name": "remove-bg",
  "description": "Smart image background removal! ðŸ¤¯",
  "services": [
    {
      "src": "src/url.ts",
      "examples": [
        {
          "name": "Girl",
          "input": {
            "url": "https://raw.githubusercontent.com/saasify-sh/saasify/master/examples/typescript/remove-bg/examples/cc.jpg"
          },
          "output": "girl-no-bg.png",
          "outputContentType": "image/png"
        },
        {
          "name": "Shoe",
          "input": {
            "url": "https://raw.githubusercontent.com/saasify-sh/saasify/master/examples/typescript/remove-bg/examples/pg.jpg"
          },
          "output": "shoe-no-bg.png",
          "outputContentType": "image/png"
        }
      ]
    },
    {
      "src": "src/base64.ts"
    },
    {
      "src": "src/raw.ts"
    }
  ],
  "env": {
    "REMOVE_BG_API_KEY": "@remove-bg-api-key"
  },
  "saas": {
    "repo": "https://github.com/saasify-sh/saasify/tree/master/examples/typescript/remove-bg",
    "logo": "media/logo.svg",
    "favicon": "media/favicon.ico"
  }
