{
  "name": "remove-bg",
  "description": "Smart image background removal",
  "amountPerBase": 999,
  "amountPerRequest": 50,
  "noAuthRateLimit": {
    "requests": true,
    "requestsInterval": 86400,
    "requestsMaxPerInterval": 5
  },
  "services": [
    {
      "src": "src/url.ts",
      "examples": [
        {
          "name": "Person",
          "input": {
            "url": "https://raw.githubusercontent.com/saasify-sh/saasify/master/examples/typescript/remove-bg/examples/cc.jpg"
          }
        },
        {
          "name": "Product",
          "input": {
            "url": "https://raw.githubusercontent.com/saasify-sh/saasify/master/examples/typescript/remove-bg/examples/pg.jpg"
          }
        },
        {
          "name": "Car",
          "input": {
            "url": "https://raw.githubusercontent.com/saasify-sh/saasify/master/examples/typescript/remove-bg/examples/jb.jpg"
          }
        }
      ]
    },
    {
      "src": "src/base64.ts"
    },
    {
      "src": "src/raw.ts"
    }
  ],
  "env": {
    "REMOVE_BG_API_KEY": "@remove-bg-api-key"
  },
  "saas": {
    "name": "Remove BG",
    "repo": "https://github.com/saasify-sh/saasify/tree/master/examples/typescript/remove-bg",
    "logo": "media/logo.svg",
    "favicon": "media/favicon.ico",
    "sections": {
      "hero": {
        "image": "https://raw.githubusercontent.com/saasify-sh/saasify/master/examples/typescript/remove-bg/media/splash-0.jpg"
      }
    }
  }
}
