{
  "name": "wordcloud",
  "description": "Generate stylistic wordclouds from any webpage.",
  "alias": "wordcloud",
  "services": [
    {
      "src": "main.py",
      "examples": [
        {
          "name": "The Onion Example",
          "input": {
            "url": "https://www.theonion.com/new-day-same-bullshit-whispers-dalai-lama-before-sl-1839720347"
          }
        }
      ]
    }
  ],
  "amountPerBase": 99,
  "amountPerRequest": 0.1,
  "amountPerCompute": 0,
  "amountPerBandwidth": 0,
  "noAuthRateLimit": {
    "requests": true,
    "requestsInterval": 3600,
    "requestsMaxPerInterval": 10
  },
  "env": {
    "DIFFBOT_TOKEN": "@diffbot-token"
  },
  "saas": {
    "name": "WordCloud",
    "heading": "**Stylistic Wordclouds**",
    "subheading": "An API that turns any webpage into a stylistic wordcloud.",
    "repo": "https://github.com/saasify-sh/wordcloud",
    "theme": {
      "name": "waves",
      "backgroundImage": "https://images.unsplash.com/photo-1495020689067-958852a7765e?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=3450&q=80",
      "buttonStyle": "rounded",
      "color": "#2097e2",
      "wave": false,
      "gradientDark": false,
      "codeBlockDark": true
    }
  }
}
